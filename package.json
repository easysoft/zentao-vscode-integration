{
	"name": "zentao",
	"displayName": "zentao",
	"description": "Zentao integration demo.",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:zentao.login",
		"onCommand:zentao.pickTasksForCommit",
		"onCommand:zentao.viewTasks"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "zentao.login",
				"title": "登录",
				"category": "禅道"
			},
			{
				"command": "zentao.pickTasksForCommit",
				"title": "选择任务形成 Commit Message",
				"category": "禅道"
			},
			{
				"command": "zentao.viewTasks",
				"title": "查看任务",
				"category": "禅道"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.62.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	},
	"dependencies": {
		"axios": "^0.24.0"
	}
}
